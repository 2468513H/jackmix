#! /usr/bin/env python

Import( 'env' )

env = env.Copy()

env.AppendUnique( CPPPATH= env['lash10_CFLAGS'] )
env.AppendUnique( CPPPATH= [ '#/libqlash' ] )
env.AppendUnique( LIBPATH= env['lash10_PATHS'] )
env.AppendUnique( LIBPATH= [ '#/libqlash'] )
env.AppendUnique( LIBS   = env['lash10_LIBS'] )
env.AppendUnique( LIBS   = [ 'qlash' ] )
env.Program( target="simple_lash_client", source=["main.cpp"] )

